<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    {% block metatags %}
        <title>{% block title %}{% endblock %}</title>
    {% endblock %}
    {% block stylesheets %}
        {% stylesheets output='assets/compiled/layout.css'
        filter='cssrewrite'
        %}
        <link href="{{ asset_url }}" rel="stylesheet" />
        {% endstylesheets %}
    {% endblock %}
    {% block headjs %}
        {% javascripts output='assets/compiled/layout_head.js'
        %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

    {% endblock %}
    {% block icons %}
        <link rel="icon" href="{{ asset('bundles/sipresource/img/favicon.ico') }}?v1.0" type="image/x-icon">
    {% endblock %}
</head>

<body>
{% include 'SIPResourceBundle::header.html.twig' %}

{% block content %}
{% endblock %}

{% block footer %}
    {% include 'SIPResourceBundle::footer.html.twig' %}
{% endblock %}

{% block bodyjs %}
    {% javascripts output='assets/compiled/layout_bod—É.js'
    %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
</body>
</html>